package Tema2;
import java.util.Scanner;
public class Ejercicio18 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int num ; 
		Scanner tecla = new Scanner(System.in);
        
		System.out.println("Escribe un numero ");
		

		do {
			num = tecla.nextInt();
		}while (num==0);
	}

}
